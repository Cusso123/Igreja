	<!-- Responsive -->
	<link rel="stylesheet" type="text/css" href="~/css/responsive.css">
</head>
<body>
		<section class="gap page-404">
		<div class="parallax" style="background-image: url(https://iili.io/Ju1OxEv.jpg);"></div>
		<div class="container">
			<div class="search-error text-center">
				<h2 class="font-bold">4<span class="position-relative theme-dark-clr">0</span>4</h2>
@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> <code>@Model.RequestId</code>
    </p>
}

				<h3 class="font-bold">Ops! Página não encontrada</h3>
				<p class="w-40 m-auto">Só falta alguma coisa, poderia ser muito pior. isso acontece com o melhor de nós.</p>
